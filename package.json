{
  "name": "spark-botkit",
  "version": "0.0.1",
  "description": "A starter kit for building a custom Cisco Spark bot with Botkit Studio",
  "main": "src/bot.js",
  "scripts": {
    "start": "node src/bot.js",
    "test": "mocha test --recursive",
    "test:watch": "npm run test -- --watch",
