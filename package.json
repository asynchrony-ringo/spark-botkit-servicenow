    "node-env-file": "^0.1.8",
    "querystring": "^0.2.0",
    "request": "^2.81.0",
    "wordfilter": "^0.2.6"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "mocha-jenkins-reporter": "^0.3.7",
    "sinon": "^2.1.0"
  },
